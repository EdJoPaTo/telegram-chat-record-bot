{
	"name": "telegram-chat-record-bot",
	"private": true,
	"version": "1.0.0",
	"description": "A Telegram Bot to record messages in a given chat for a period of time",
	"license": "MIT",
	"repository": "EdJoPaTo/telegram-chat-record-bot",
	"author": {
		"name": "EdJoPaTo",
		"email": "telegram-chat-record-bot@edjopato.de",
		"url": "https://edjopato.de"
	},
	"engines": {
		"node": ">=8"
	},
	"scripts": {
		"build": "del dist && tsc",
		"prepack": "npm run build",
		"start": "ts-node source/index.ts",
		"test": "tsc && xo"
	},
	"main": "dist",
	"types": "dist",
	"files": [
		"dist",
		"!*.test.*"
	],
	"keywords": [
		""
	],
	"dependencies": {
		"@edjopato/datastore": "^0.2.1",
		"source-map-support": "^0.5.16",
		"telegraf": "^3.33.3",
		"telegraf-i18n": "^6.6.0",
		"telegraf-middleware-console-time": "^1.0.0",
		"telegram-typings": "^3.6.1"
	},
	"devDependencies": {
		"@sindresorhus/tsconfig": "^0.7.0",
		"@types/node": "^13.9.5",
		"del-cli": "^3.0.0",
		"ts-node": "^8.4.1",
		"typescript": "^3.3.3",
		"xo": "^0.29.0"
	},
	"xo": {
		"semicolon": false,
		"rules": {
			"@typescript-eslint/no-unsafe-member-access": "warn",
			"@typescript-eslint/no-unsafe-return": "warn",
			"@typescript-eslint/prefer-readonly-parameter-types": "off"
		}
	}
}
